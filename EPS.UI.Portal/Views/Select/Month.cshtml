@using EPS.UI.Portal.Models
<link href="/wwwroot/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
<link href="/wwwroot/lib/AmazeUI/css/amazeui.css" rel="stylesheet" />

@{
    var monthList = ViewBag.MonthList as List<SelectListItem> ?? new List<SelectListItem>();
    string name = ViewBag.Name;
}

<select data-am-selected id="monthSelection" name="@name">
    @Html.Partial("component/SelectOptions", monthList)
</select>

